datasource DS {
  provider = "postgresql"
  url      = "postgresql://postgres:raspberry@192.168.0.71/cookit"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

// Define your own datamodels here and run `yarn redwood db save` to create
// migrations for them.
model Recipe {
  // simple model for a recipe
  id          Int       @default(autoincrement()) @id
  title       String?
  duration    String?
  ingredients String?
  image       String?
  author      String?
  preparation String?
  favorite    Boolean?
  created_at  DateTime? @default(now())
}
